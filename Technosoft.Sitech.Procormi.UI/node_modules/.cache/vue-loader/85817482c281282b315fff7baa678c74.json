  Cookies.set(\"dpto\", UsuCookie)\r\n            },\r\n\r\n            guardarCat(UsuCookie) {\r\n                Cookies.set(\"cat\", UsuCookie)\r\n            },\r\n\r\n            guardarSubCat(UsuCookie) {\r\n                Cookies.set(\"sCat\", UsuCookie)\r\n            },\r\n\r\n            obtenerDpto: function () {\r\n                this.guardarDpto(400)\r\n            },\r\n\r\n            obtenerCat: function () {\r\n                this.guardarCat(400)\r\n            },\r\n\r\n            obtenerSubCat: function () {\r\n                this.guardarSubCat(400)\r\n            },\r\n\r\n            recuperarDpto() {\r\n                return Cookies.get(\"dpto\")\r\n            },\r\n\r\n            recuperarCat() {\r\n                return Cookies.get(\"cat\")\r\n            },\r\n\r\n            recuperarSubCat() {\r\n                return Cookies.get(\"sCat\")\r\n            },\r\n\r\n            recuperarUsuTipo() {\r\n                return Cookies.get(\"tipoU\")\r\n            },\r\n\r\n            TotalTemas: function () {\r\n                AdminApi.GetAdminTotalTemas()\r\n                    .then(response => {\r\n                        if (response.data != null) {\r\n                            this.totalTemas = response.data\r\n                        }\r\n                    })\r\n                return this.totalTemas\r\n            },\r\n\r\n            totalPaginas: function () {\r\n                return Math.ceil(this.TotalTemas() / this.elementosPorPaginaPrincipal)\r\n            },\r\n\r\n            ListarPublicacionPaginadoOriginal: function (sCat) {\r\n                this.guardarSubCat(sCat);\r\n            },\r\n\r\n            ListarPublicacionPaginado: async function (dpto, cat, sCat, elementosPorPagina, offset) {\r\n                let loader = this.$loading.show({\r\n                    opacity: 1\r\n                })\r\n                this.guardarDpto(dpto);\r\n                this.guardarCat(cat);\r\n                this.guardarSubCat(sCat);\r\n                var departamentoId = this.recuperarDpto();\r\n                var categoriaId = this.recuperarCat();\r\n                var subCategoriaId = this.recuperarSubCat();\r\n                var usuTipo = this.recuperarUsuTipo();\r\n                this.ListaPublicacionTemporal = []\r\n                elementosPorPagina = this.elementosPorPaginaPrincipal\r\n                offset = 0\r\n                await this.$root.limpiarFiltroP.call();\r\n\r\n                if (departamentoId == 400 && categoriaId == 400 && subCategoriaId == 400) {\r\n                    window.location.reload()\r\n                } else if (usuTipo != \"Administrador\") {\r\n                    if (dpto != 400 || cat != 400 || sCat != 400) {\r\n                        await AdminApi.GetAdminPaginarPublicacionesAprobadasSubcategoria(dpto, cat, sCat, this.elementosPorPaginaPrincipal, offset)\r\n                            .then(response => {\r\n                                if (response.data != null) {\r\n                                    this.guardarSubCat(sCat);\r\n                                    if (dpto == 400 && cat == 400 && sCat == 400) {\r\n                                        window.location.reload()\r\n                                    } else {\r\n                                        this.FiltrarSubcategoria([])\r\n                                        response.data.obj.forEach(element => {\r\n                                            this.ListaPublicaciones.push({\r\n                                                id_know_tema: element.id_know_tema,\r\n                                                id_gen_departamento: element.id_gen_departamento,\r\n                                                id_know_categoria: element.id_know_categoria,\r\n                                                id_know_subcategoria: element.id_know_subcategoria,\r\n                                                know_tema_titulo: element.know_tema_titulo,\r\n                                                know_tema_subtitulo: element.know_tema_subtitulo,\r\n                                                know_tema_palabraClave: element.know_tema_palabraClave,\r\n                                                know_tema_contenido: this.QuitarHTML(element.know_tema_contenido),\r\n                                                know_tema_tipoCliente: element.know_tema_tipoCliente,\r\n                                                know_tema_cantConsul: element.know_tema_cantConsul,\r\n                                                know_tema_estado: element.know_tema_estado,\r\n                                                know_tema_login: element.know_tema_login,\r\n                                                know_tema_nombreUsu: element.know_tema_nombreUsu,\r\n                                            })\r\n                                        })\r\n                                    }\r\n                                }\r\n                            }\r\n                            )\r\n                    }\r\n                } else {\r\n                    await AdminApi.FiltrarTemaSubcategoriaPaginado(dpto, cat, sCat, this.elementosPorPaginaPrincipal, offset)\r\n                        .then(response => {\r\n                            if (response.data != null) {\r\n                                this.guardarSubCat(sCat);\r\n                                if (dpto == 400 && cat == 400 && sCat == 400) {\r\n                                    window.location.reload()\r\n                                } else {\r\n                                    this.FiltrarSubcategoria([])\r\n                                    response.data.obj.forEach(element => {\r\n                                        this.ListaPublicaciones.push({\r\n                                            id_know_tema: element.id_know_tema,\r\n                                            id_gen_departamento: element.id_gen_departamento,\r\n                                            id_know_categoria: element.id_know_categoria,\r\n                                            id_know_subcategoria: element.id_know_subcategoria,\r\n                                            know_tema_titulo: element.know_tema_titulo,\r\n                                            know_tema_subtitulo: element.know_tema_subtitulo,\r\n                                            know_tema_palabraClave: element.know_tema_palabraClave,\r\n                                            know_tema_contenido: this.QuitarHTML(element.know_tema_contenido),\r\n                                            know_tema_tipoCliente: element.know_tema_tipoCliente,\r\n                                            know_tema_cantConsul: element.know_tema_cantConsul,\r\n                                            know_tema_estado: element.know_tema_estado,\r\n                                            know_tema_login: element.know_tema_login,\r\n                                            know_tema_nombreUsu: element.know_tema_nombreUsu,\r\n                                        })\r\n                                    })\r\n                                }\r\n                            }\r\n                        })\r\n                }\r\n                await this.$root.ActualizarPaginas.call();\r\n                await this.$root.ReiniciarPagina.call();\r\n                loader.hide()\r\n            },\r\n\r\n            FiltrarPublicaciones: function (sCat) {\r\n                if (sCat == 400) {\r\n                    this.ListarPublicacionPaginado2(4, 1)\r\n                }\r\n            },\r\n\r\n            ListarPublicacionPaginado2: function (elementosPorPagina, offset) {\r\n                if (offset == 0) {\r\n                    this.paginaActual = 1\r\n                } else {\r\n                    this.paginaActual = offset\r\n                }\r\n                elementosPorPagina = this.elementosPorPaginaPrincipal\r\n                offset = 4 * offset - 4\r\n                AdminApi.GetAdminPaginarPublicaciones(this.elementosPorPaginaPrincipal, offset)\r\n                    .then(response => {\r\n                        if (response.data != null) {\r\n                            this.AsignarPublicaciones(response.data.obj)\r\n                        }\r\n                    })\r\n            },\r\n\r\n            ListarPublicacion: function () {\r\n                AdminApi.GetAdminListarTema()\r\n                    .then(response => {\r\n                        if (response.data != null) {\r\n                            this.FiltrarSubcategoria(response.data.obj)\r\n                        }\r\n                    })\r\n            },\r\n\r\n            ListarDepartamento: function () {\r\n                AdminApi.GetAdminListarDepartamentos()\r\n                    .then(response => {\r\n                        if (response.data != null) {\r\n                            response.data.obj.forEach(element => {\r\n                                this.ListaDepartamentos.push({\r\n                                    id_gen_departamento: element.id_gen_departamento,\r\n                                    nombre: this.cortarNombre(element.nombre),\r\n                                    correo: element.correo\r\n                                })\r\n                            })\r\n                        }\r\n                    })\r\n            },\r\n\r\n            ListarCategoria: function () {\r\n                AdminApi.GetAdminListarCategorias()\r\n                    .then(response => {\r\n                        if (response.data != null) {\r\n                            this.ListaCategorias = []\r\n                            response.data.obj.forEach(element => {\r\n                                this.ListaCategorias.push({\r\n                                    id_know_categoria: element.id_know_categoria,\r\n                                    id_gen_departamento: element.id_gen_departamento,\r\n                                    know_categoria_nombre: this.cortarNombre(element.know_categoria_nombre),\r\n                                })\r\n                            })\r\n                        }\r\n                    })\r\n            },\r\n\r\n            ListarSubcategoria: function () {\r\n                AdminApi.GetAdminListarSubcategorias()\r\n                    .then(response => {\r\n                        if (response.data != null) {\r\n                            this.ListaSubcategorias = []\r\n                            response.data.obj.forEach(element => {\r\n                                this.ListaSubcategorias.push({\r\n                                    id_know_subcategoria: element.id_know_subcategoria,\r\n                                    id_know_categoria: element.id_know_categoria,\r\n                                    know_subcategoria_nombre: this.cortarNombre(element.know_subcategoria_nombre),\r\n                                })\r\n                            })\r\n                        }\r\n                    })\r\n            },\r\n\r\n            FiltrarCategorias: function (dpto) {\r\n                if (dpto == \"\" || dpto == null) {\r\n                    dpto = 400\r\n                }\r\n                AdminApi.selectCategoria(dpto)\r\n                    .then(response => {\r\n\r\n                        this.LimpiarSubcategorias()\r\n\r\n                        if (dpto == 400) {\r\n                            this.ListarCategoria()\r\n                        } else if (response.data != null) {\r\n                            this.ListaCategorias = response.data.obj\r\n                        }\r\n                    })\r\n            },\r\n\r\n            LimpiarCategorias: function () {\r\n                this.ListaCategorias = [],\r\n                this.Filtro.categoria = 400\r\n                this.ListaSubcategorias = [],\r\n                this.Filtro.subcategoria = 400\r\n            },\r\n\r\n            LimpiarSubcategorias: function () {\r\n                this.ListaSubcategorias = [],\r\n                this.Filtro.subcategoria = 400\r\n            },\r\n\r\n            FiltrarSubcategorias: function (cat, dpto) {\r\n                \r\n                if (cat == \"\" || cat == null) {\r\n                    cat = 400\r\n                }\r\n                if (dpto == \"\" || dpto == null) {\r\n                    dpto = 400\r\n                }\r\n\r\n                AdminApi.selectSubcategoria(cat, dpto)\r\n                    .then(response => {\r\n\r\n                        if (cat == 400 && dpto == 400) {\r\n                            this.ListarSubcategoria()\r\n                        } else if (response.data != null) {\r\n                            this.ListaSubcategorias = response.data.obj\r\n                        }\r\n                    })\r\n            },\r\n\r\n            ContTemp: function (element) {\r\n                this.ListaPublicacionTemporal.push({\r\n                    id_know_tema: element.id_know_tema,\r\n                    id_gen_departamento: element.id_gen_departamento,\r\n                    id_know_categoria: element.id_know_categoria,\r\n                    id_know_subcategoria: element.id_know_subcategoria,\r\n                    know_tema_titulo: element.know_tema_titulo,\r\n                    know_tema_subtitulo: element.know_tema_subtitulo,\r\n                    know_tema_palabraClave: element.know_tema_palabraClave,\r\n                    know_tema_contenido: element.know_tema_contenido,\r\n                    know_tema_tipoCliente: element.know_tema_tipoCliente,\r\n                    know_tema_cantConsul: element.know_tema_cantConsul,\r\n                    know_tema_estado: element.know_tema_estado,\r\n                })\r\n            },\r\n\r\n            QuitarHTML(html) {\r\n                var temporal = document.createElement('div')\r\n                temp