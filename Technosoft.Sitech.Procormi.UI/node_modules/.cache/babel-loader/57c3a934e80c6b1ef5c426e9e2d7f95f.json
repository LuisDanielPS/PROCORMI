       <p style=\"text-align: center; font-size: 15px; color: #000000;\"><b>Link:</b> <a>{{ fullLink }}</a></p>\r\n                                    <div>\r\n                                        <div>\r\n                                            <div class=\"cardPersonalizado\" v-for=\"(question, index) in poll.Questions\" :key=\"index\">\r\n                                                <div class=\"cardInside\">\r\n                                                    <h5 style=\"text-align: center; font-size: 25px; background-color: #0a3a66; color: #fff; border-radius: 15px;\">{{ question.TextQuestion }}</h5>\r\n                                                    <hr />\r\n                                                    <input v-if=\"question.Id_Question_Type == 1\" style=\"border-radius: 15px; margin-bottom: 10px;\" placeholder=\"Respuesta\" />\r\n                                                    <br />\r\n                                                    <br />\r\n                                                    <div v-if=\"question.Id_Question_Type == 2 || question.Id_Question_Type == 3\">\r\n                                                        <h5 class=\"margin-5px-bottom\" style=\"font-size: 20px; color: #0a3a66;\">Opciones</h5>\r\n                                                        <br />\r\n                                                        <div v-if=\"question.Id_Question_Type == 2\">\r\n                                                            <div class=\"form-check\" v-for=\"(option, index) in question.Question_Options\" :key=\"index\">\r\n                                                                <input type=\"radio\" class=\"estiloRadios\" :id=\"'opcion' + index\" name=\"options\" v-model=\"selectedOption\">\r\n                                                                <label class=\"form-check-label\">\r\n                                                                    {{ option.Option_Text }}\r\n                                                                </label>\r\n                                                            </div>\r\n                                                        </div>\r\n                                                        <div v-if=\"question.Id_Question_Type == 3\">\r\n                                                            <div class=\"form-check\" v-for=\"(option, index) in question.Question_Options\" :key=\"index\">\r\n                                                                <input type=\"checkbox\" class=\"estiloRadios\" id=\"opcion1\">\r\n                                                                <label class=\"form-check-label\">\r\n                                                                    {{ option.Option_Text }}\r\n                                                                </label>\r\n                                                            </div>\r\n                                                        </div>\r\n                                                    </div>\r\n                                                </div>\r\n                                            </div>\r\n                                            \r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <!--Preguntas-->\r\n\r\n                                <br />\r\n                                <br />\r\n                                <div class=\"row justify-content-end\">\r\n                                    <div class=\"col-2\">\r\n                                        <button class=\"btn btn-success\" style=\"min-width: 120px; font-size: 20px; float: right;\">Enviar respuestas</button>\r\n                                    </div>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <br />\r\n                <br />\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderPrincipal from '@/components/HeaderPrincipal.vue'\r\nimport 'quill/dist/quill.snow.css'\r\nimport Cookies from 'js-cookie';\r\nimport AdminApi from '@/Api/Api';\r\n\r\nexport default {\r\n\r\n    components: {\r\n        HeaderPrincipal\r\n    },\r\n\r\n    data() {\r\n        return {\r\n            poll: [],\r\n            link: \"\",\r\n            fullLink: \"\"\r\n        }\r\n    },\r\n\r\n    methods: {\r\n\r\n        getPoll: async function () {\r\n            let id = this.$route.params.encuesta\r\n            await AdminApi.getLink()\r\n                .then(async response => {\r\n                    if (response.data != null) {\r\n                        this.link = response.data\r\n                    }\r\n                })\r\n\r\n            this.fullLink = this.link + \"/\" + id\r\n\r\n            await AdminApi.GetPoll(id)\r\n                .then(async response => {\r\n                    if (response.data != null) {\r\n                        this.poll = response.data\r\n                    }\r\n                })\r\n\r\n            console.log(this.poll)\r\n        },\r\n\r\n        QuitarHTML(html) {\r\n            var temporal = document.createElement('div')\r\n            temporal.innerHTML = html\r\n            return temporal.textContent || temporal.innerText || \"\"\r\n        },\r\n\r\n        limpiarEditor: function () {\r\n            this.quill.root.innerHTML = \"\"\r\n        },\r\n\r\n        recuperarUsuLog() {\r\n            return Cookies.get(\"usuarioLogin\")\r\n        },\r\n\r\n        recuperarUsuTipo() {\r\n            return Cookies.get(\"tipoU\")\r\n        },\r\n\r\n        recuperarUsuNombre() {\r\n            return Cookies.get(\"nombreUsu\")\r\n        },\r\n\r\n        verificarLog: function () {\r\n            let login = this.recuperarUsuLog()\r\n            let nombre = this.recuperarUsuNombre()\r\n            if (login == null || (nombre == undefined || nombre == null || nombre == '')) {\r\n                this.$router.push(\"/\");\r\n                this.$swal({ icon: 'warning', text: 'Aún no ha iniciado sesión, por favor verifique' });\r\n            }\r\n\r\n        },\r\n\r\n    },\r\n\r\n    mounted: async function () {\r\n        let loader = this.$loading.show({\r\n            opacity: 1\r\n        })\r\n        setTimeout(() => {\r\n            loader.hide()\r\n        }, 500)\r\n    },\r\n\r\n    created: async function () {\r\n        await this.getPoll()\r\n        await this.$root.validarLoginFooter.call();\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.inputsGeneral{\r\n    border-radius: 15px;\r\n    min-height: 50px;\r\n}\r\n\r\n.BotonesJ {\r\n    display: flex;\r\n    flex-direction: row-reverse;\r\n    color: white;\r\n    top: 0;\r\n    width: 100%;\r\n    align-content: center;\r\n}\r\n\r\n    .BotonesJ div {\r\n        margin-left: 10px;\r\n        min-width: 80px;\r\n    }\r\n\r\n.Adj {\r\n    display: grid;\r\n    grid-template-columns: 75% 25%;\r\n    grid-row-gap: 25px;\r\n}\r\n\r\n.agregarAdj {\r\n    display: grid;\r\n    grid-template-columns: 30% 40% 30%;\r\n    grid-row-gap: 25px;\r\n}\r\n\r\n#textBox {\r\n    width: 600px;\r\n    height: 300px;\r\n    border: 2px #000000 solid;\r\n    padding: 10px;\r\n    overflow: scroll;\r\n    background-color: yellow;\r\n}\r\n\r\n.ql-editor {\r\n    height: 40vh;\r\n    padding-left: 0 !important;\r\n    padding-right: 0 !important;\r\n    padding-bottom: 70px !important;\r\n    overflow-y: visible !important;\r\n}\r\n\r\n.selectsJ {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    padding-left: 0px !important;\r\n}\r\n\r\n    .selectsJ div {\r\n        padding-left: 0px !important;\r\n    }\r\n\r\n@media screen and (max-width: 991px) {\r\n    .selectsJ {\r\n        display: grid;\r\n        grid-template-columns: 100%;\r\n    }\r\n}\r\n\r\n@media screen and (max-width: 800px) {\r\n\r\n    .quillResponsiveCelular {\r\n        padding-bottom: 100px;\r\n    }\r\n}\r\n\r\n.fondoEncuestas{\r\n    background: rgba(238,244,255,1);\r\n    min-height: calc(100vh - 50px);\r\n    /*background: linear-gradient(90deg, rgba(208,223,255,1) 0%, rgba(238,244,255,1) 50%, rgba(208,223,255,1) 100%);*/\r\n}\r\n\r\n.estiloForm{\r\n    background: white;\r\n    border-radius: 25px;\r\n    margin-right: 50PX;\r\n    margin-left: 50px;\r\n}\r\n\r\n.botones{\r\n    min-height: 40px;\r\n    color: white;\r\n    min-width: 110px;\r\n}\r\n\r\n.cardPersonalizado{\r\n    border: solid 1px rgb(184, 184, 184);\r\n    border-radius: 20px;\r\n    box-shadow: 5px 5px 10px rgb(176, 196, 233);\r\n    transition: box-shadow 0.3s;\r\n    margin-top: 35px;\r\n}\r\n\r\n.cardPersonalizado:hover{\r\n    box-shadow: 5px 5px 15px rgb(152, 176, 219);\r\n}\r\n\r\n.cardInside{\r\n    padding: 15px;\r\n}\r\n\r\n.estiloRadios {\r\n    margin-right: 10px;\r\n}\r\n\r\n.labelGen {\r\n    color: #000000;\r\n}\r\n\r\n</style>"],"mappings":";;;EACSA,KAAK,EAAC;AAA+B;;EAG7BA,KAAK,EAAC,QAAQ;EAACC,KAAyB,EAAzB;IAAA;EAAA;;gEAChBC,mBAAA,CAAwF;EAApFD,KAA4D,EAA5D;IAAA;IAAA;IAAA;EAAA;AAA4D,GAAC,oBAAkB;gEACnFC,mBAAA,CAAM;;EAGOF,KAAK,EAAC;AAAY;;EACdC,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EAKfA,KAA4D,EAA5D;IAAA;IAAA;IAAA;EAAA;AAA4D;gEAChEC,mBAAA,CAAI;;EACDD,KAA4D,EAA5D;IAAA;IAAA;IAAA;EAAA;AAA4D;iEAC/DC,mBAAA,CAAI;;EACDD,KAA4D,EAA5D;IAAA;IAAA;IAAA;EAAA;AAA4D;iEAACC,mBAAA,CAAY,WAAT,OAAK;;;EAIvDF,KAAK,EAAC;AAAY;;EACfC,KAAyG,EAAzG;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAAyG;iEAC7GC,mBAAA,CAAM;;;EACuCD,KAAiD,EAAjD;IAAA;IAAA;EAAA,CAAiD;EAACE,WAAW,EAAC;;iEAC3GD,mBAAA,CAAM;iEACNA,mBAAA,CAAM;;;;iEAEFA,mBAAA,CAAoF;EAAhFF,KAAK,EAAC,mBAAmB;EAACC,KAAwC,EAAxC;IAAA;IAAA;EAAA;GAAyC,UAAQ;iEAC/EC,mBAAA,CAAM;;;;;;EAISF,KAAK,EAAC;AAAkB;;;;iEAO/BE,mBAAA,CAAyD;EAAlDE,IAAI,EAAC,UAAU;EAACJ,KAAK,EAAC,cAAc;EAACK,EAAE,EAAC;;;EACxCL,KAAK,EAAC;AAAkB;iEAe/DE,mBAAA,CAAM;iEA