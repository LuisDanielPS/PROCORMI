","aoa_to_sheet","book_append_sheet","blob","write","bookType","type","url","window","URL","createObjectURL","a","document","createElement","href","download","style","display","body","appendChild","click","revokeObjectURL","mounted","cerrarMenuFiltros","cerrarFiltros","loader","$loading","show","container","$refs","cuadroLoader","opacity","setTimeout","hide","created","validarLoginFooter"],"sources":["C:\\Users\\crist\\source\\repos\\LuisDanielPS\\PROCORMI\\Technosoft.Sitech.Procormi.UI\\src\\views\\Reportes\\Reportes.vue"],"sourcesContent":["<template>\r\n    <div class=\"vistaPrincipal\" style=\"min-height: calc(100vh - 50px);\">\r\n        <!--<HeaderPrincipal :cuadroLoaderPrincipal=\"$refs.cuadroLoader\" />-->\r\n        <HeaderPrincipal />\r\n\r\n        <div class=\"posicion0\" v-on:click=\"cerrarMenu()\">\r\n            <div class=\"d-flex\">\r\n                <MenuLateral />\r\n\r\n\r\n                <div class=\"modal fade\" id=\"listprojectreport\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\"\r\n                    aria-hidden=\"true\">\r\n                    <div class=\"modal-dialog\">\r\n                        <div class=\"modal-content\">\r\n                            <div class=\"modal-header\">\r\n                                <h1 class=\"modal-title fs-5\" id=\"exampleModalLabel\">Generar reporte de proyecto por usuario</h1>\r\n                                <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n                            </div>\r\n                            <div class=\"modal-body\">\r\n                                <div class=\"content-icon\">\r\n\r\n                                    <span class=\"fas fa-file-excel\" ></span>  \r\n\r\n                                </div>\r\n\r\n                                <div>\r\n                                    <label>Busque el usuario del que desea generar el reporte</label>\r\n                                    <br />\r\n                                    <div class=\"row\" style=\"margin-top: 15px;\">\r\n                                        <select v-model=\"selectedUser\" required name=\"usuarios\" id=\"usuarios\"\r\n                                                class=\"form-select text-black inputsGeneral\" style=\"min-height: 48px;\">\r\n                                                <option :value=\"null\">Seleccione una opción</option>\r\n                                                <option v-for=\"item in listUsers\" :key=\"item.usu_Login\"\r\n                                                    :value=\"JSON.stringify(item)\">{{ item.usu_Nombre }}</option>\r\n                                         </select>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"modal-footer\">\r\n                                <button type=\"button\" class=\"btn btn-danger\" data-bs-dismiss=\"modal\">Cancelar</button>\r\n                                <button  class=\"btn btn-success\" @click=\"descargarExcel()\">Generar</button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n\r\n                <div class=\"w-100 listadoGeneral\">\r\n\r\n                    <!--Filtros responsive /-->\r\n\r\n                    <div class=\"row justify-content-center filtrosCelularBotones\">\r\n                        <div class=\"col-5\"></div>\r\n                        <div class=\"col-2 filtrosCelularBoton\" v-if=\"!filtroDesplegar\" v-on:click=\"desplegarFiltros()\">\r\n                            <a class=\"text-gradient-yellow-orange-black fas fa-bars\"></a>\r\n                        </div>\r\n                        <div class=\"col-5\"></div>\r\n                    </div>\r\n\r\n                    <div class=\"filtrosCelular row justify-content-center\" v-if=\"filtroDesplegar\">\r\n\r\n                        <div class=\"col-lg-4 col-md-4 col-sm-12 row justify-content-center\" style=\"margin-top: 40px;\">\r\n                            <div class=\"col-8 textoBlanco\" style=\"text-align: center;\">\r\n                                <router-link role=\"button\" :to=\"{ name: 'Inicio' }\" class=\"textoBlanco textoEncuestas\" style=\"text-decoration: none;\" exact-active-class=\"active-link\">Proyectos&nbsp;&nbsp;<i class=\"text-white fas fa-project-diagram\" style=\"cursor: pointer; font-size: 14px;\"></i></router-link>\r\n                            </div>\r\n                        </div>\r\n                        \r\n                        <div class=\"col-lg-4 col-md-4 col-sm-12 row justify-content-center\" style=\"margin-top: 40px;\">\r\n                            <div class=\"col-8 textoBlanco\" style=\"text-align: center;\">\r\n                                <router-link role=\"button\" :to=\"{ name: 'Encuestas' }\" class=\"textoBlanco textoEncuestas\" style=\"text-decoration: none;\" exact-active-class=\"active-link\">Encuestas&nbsp;&nbsp;<i class=\"text-white fas fa-chart-line\" style=\"cursor: pointer;\"></i></router-link>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div class=\"col-lg-4 col-md-4 col-sm-12 row justify-content-center\" style=\"margin-top: 40px;\">\r\n                            <div class=\"col-8 textoBlanco\" style=\"text-align: center;\">\r\n                                <router-link role=\"button\" :to=\"{ name: 'Reportes' }\" class=\"textoBlanco textoEncuestas\" style=\"text-decoration: none;\" exact-active-class=\"active-link\">Reportes&nbsp;&nbsp;<i class=\"text-white far fa-file-alt\" style=\"cursor: pointer;\"></i></router-link>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div style=\"text-align: center; font-size: large; padding-top: 40px; cursor: pointer; margin-left: 20px;\" v-if=\"filtroDesplegar\" v-on:click=\"desplegarFiltros()\">\r\n                            <a class=\"text-white fas fa-angle-up\" style=\"text-decoration: none;\"></a>\r\n                        </div>\r\n\r\n                    </div>\r\n\r\n                    <!--Filtros responsive /-->\r\n\r\n                    <div class=\"w-100 vld-parent col-12\" style=\"min-height: 85vh;\">\r\n\r\n                        <!--Lista de reportes /-->\r\n\r\n                        <div class=\"row\" style=\"padding:15px; min-height: 95vh; padding-right: 45px;\">\r\n                            <div class=\"col-12 estiloTabla\" style=\"padding:15px;\">\r\n                                <div class=\"card\" style=\"border: none;\" ref=\"cuadroLoader\">\r\n                                    <div class=\"encabezado\">\r\n                                        <ul style=\"text-align: left;\">\r\n                                            <router-link class=\"li agregarBlt\" role=\"button\" :to=\"{ name: 'Inicio' }\"><span class=\"fas fa-arrow-left\"></span> <span class=\"linkResponsiveCelular\"> Inicio</span></router-link>\r\n                                        </ul>\r\n                                        <div style=\"text-align: center; padding-top: 10px; cursor: default;\">\r\n                                            <h4>Listado de Reportes</h4>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"contenidoTabla\">\r\n                                        <table class=\"table table-striped table-bordered\" style=\"text-align: left;\">\r\n                                            <thead>\r\n                                                <tr>\r\n                                                    <th class=\"col-10\" style=\"min-width: 150px;\">Descripción</th>\r\n                                                    <th class=\"col-2\" style=\"min-width: 125px;\">Opciones</th>\r\n                                                </tr>\r\n                                            </thead>\r\n                                            <tbody style=\"font-size: large;\">\r\n                                                <tr>\r\n                                                    <td>Listado de proyectos por usuario</td>\r\n                                                    <td class=\"text-white\">\r\n                                                        <button data-bs-toggle=\"modal\" data-bs-target=\"#listprojectreport\" type=\"button\" class=\"btn btn-primary\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-download\" b-tooltip.hover title=\"Excel\" ></span>\r\n                                                        </button>\r\n                                                        <!--<button class=\"btn btn-success\" role=\"button\" @click=\"verSprints\">\r\n                                                            <span class=\"fas fa-file-excel\" b-tooltip.hover title=\"EXCEL\"></span>\r\n                                                        </button>\r\n                                                        <button style=\"margin-left: 5px;\" type=\"button\" class=\"btn btn-primary\">\r\n                                                            <span class=\"fas fa-file-word\" b-tooltip.hover title=\"WORD\"></span>\r\n                                                        </button>\r\n                                                        <button type=\"button\" class=\"btn btn-danger\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-pdf\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>-->\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <td>Listado de sprints por usuario</td>\r\n                                                    <td class=\"text-white\">\r\n                                                        <button type=\"button\" class=\"btn btn-primary\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-download\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>\r\n                                                        <!--<button class=\"btn btn-success\" role=\"button\" @click=\"verSprints\">\r\n                                                            <span class=\"fas fa-file-excel\" b-tooltip.hover title=\"EXCEL\"></span>\r\n                                                        </button>\r\n                                                        <button style=\"margin-left: 5px;\" type=\"button\" class=\"btn btn-primary\">\r\n                                                            <span class=\"fas fa-file-word\" b-tooltip.hover title=\"WORD\"></span>\r\n                                                        </button>\r\n                                                        <button type=\"button\" class=\"btn btn-danger\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-pdf\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>-->\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <td>Listado de tareas por usuario</td>\r\n                                                    <td class=\"text-white\">\r\n                                                        <button type=\"button\" class=\"btn btn-primary\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-download\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>\r\n                                                        <!--<button class=\"btn btn-success\" role=\"button\" @click=\"verSprints\">\r\n                                                            <span class=\"fas fa-file-excel\" b-tooltip.hover title=\"EXCEL\"></span>\r\n                                                        </button>\r\n                                                        <button style=\"margin-left: 5px;\" type=\"button\" class=\"btn btn-primary\">\r\n                                                            <span class=\"fas fa-file-word\" b-tooltip.hover title=\"WORD\"></span>\r\n                                                        </button>\r\n                                                        <button type=\"button\" class=\"btn btn-danger\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-pdf\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>-->\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <td>Listado de subtareas por usuario</td>\r\n                                                    <td class=\"text-white\">\r\n                                                        <button type=\"button\" class=\"btn btn-primary\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-download\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>\r\n                                                        <!--<button class=\"btn btn-success\" role=\"button\" @click=\"verSprints\">\r\n                                                            <span class=\"fas fa-file-excel\" b-tooltip.hover title=\"EXCEL\"></span>\r\n                                                        </button>\r\n                                                        <button style=\"margin-left: 5px;\" type=\"button\" class=\"btn btn-primary\">\r\n                                                            <span class=\"fas fa-file-word\" b-tooltip.hover title=\"WORD\"></span>\r\n                                                        </button>\r\n                                                        <button type=\"button\" class=\"btn btn-danger\" style=\"margin-left: 5px;\">\r\n                                                            <span class=\"fas fa-file-pdf\" b-tooltip.hover title=\"PDF\"></span>\r\n                                                        </button>-->\r\n                                                    </td>\r\n                                                </tr>\r\n                                            </tbody>\r\n                                        </table>\r\n\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <!--Lista de reportes /-->\r\n\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport Cookies from 'js-cookie';\r\nimport HeaderPrincipal from '@/components/HeaderPrincipal.vue'\r\nimport MenuLateral from '@/components/MenuLateral.vue'\r\nimport AdminApi from '@/Api/Api';\r\nimport * as XLSX from 'xlsx';\r\nexport default {\r\n\r\n    components: {\r\n        HeaderPrincipal, MenuLateral\r\n    },\r\n\r\n    data() {\r\n        return {\r\n            filtroDesplegar: false,\r\n            selectedUser:null,\r\n            listUsers:[],\r\n\r\n        }\r\n    },\r\n\r\n    methods: {\r\n\r\n        desplegarFiltros: function () {\r\n            this.filtroDesplegar = !this.filtroDesplegar;\r\n        },\r\n\r\n        cerrarMenu: async function () {\r\n            await this.$root.CerrarMenu.call();\r\n            await this.$root.CerrarMenuAplicaciones.call();\r\n        },\r\n\r\n        recuperarUsuLog() {\r\n            return Cookies.get(\"usuarioLogin\")\r\n        },\r\n\r\n        recuperarUsuTipo() {\r\n            return Cookies.get(\"tipoU\")\r\n        },\r\n\r\n        recuperarUsuNombre() {\r\n            return Cookies.get(\"nombreUsu\")\r\n        },\r\n\r\n        verificarLog: function () {\r\n            let login = this.recuperarUsuLog()\r\n            let nombre = this.recuperarUsuNombre()\r\n            if (login == null || (nombre == undefined || nombre == null || nombre == '')) {\r\n                this.$router.push(\"/\");\r\n                this.$swal({ icon: 'warning', text: 'Aún no ha iniciado sesión, por favor verifique' });\r\n            }\r\n\r\n        },\r\n        \r\n        loadUserSelect: async function () {\r\n            try {\r\n                const response = await AdminApi.GetALLUsers();\r\n                const userList = response.data.obj;\r\n                this.listUsers = userList;\r\n            } catch (error) {\r\n                console.error('Error al cargar los proyectos desde la API:', error);\r\n            }\r\n\r\n        },\r\n      descargarExcel:function() {\r\n      // Crear un conjunto de datos\r\n      const data = [\r\n        ['Nombre', 'Edad'],\r\n        ['Juan', 30],\r\n        ['María', 25],\r\n        ['Pedro', 35],\r\n      ];\r\n\r\n      // Crear un libro de trabajo y agregar una hoja\r\n      const workbook = XLSX.utils.book_new();\r\n      const worksheet = XLSX.utils.aoa_to_sheet(data);\r\n      XLSX.utils.book_append_sheet(workbook, worksheet, 'Hoja1');\r\n\r\n      // Generar un archivo Blob a partir del libro de trabajo\r\n      const blob = XLSX.write(workbook, { bookType: 'xlsx', type: 'blob' });\r\n\r\n      // Crear una URL para el Blob\r\n      const url = window.URL.createObjectURL(blob);\r\n\r\n      // Crear un enlace temporal y simular un clic para descargar\r\n      const a = document.createElement('a');\r\n      a.href = url;\r\n      a.download = 'ejemplo.xlsx';\r\n      a.style.display = 'none';\r\n      document.body.appendChild(a);\r\n      a.click();\r\n\r\n      // Limpia el enlace temporal\r\n      window.URL.revokeObjectURL(url);\r\n\r\n    },\r\n},\r\n\r\n    mounted: async function() {\r\n        this.$root.cerrarMenuFiltros = this.cerrarFiltros;\r\n        let loader = this.$loading.show({\r\n            container: this.$refs.cuadroLoader,\r\n            opacity: 1\r\n        })\r\n                \r\n        setTimeout(() => {\r\n            loader.hide()\r\n        }, 500)\r\n    },\r\n\r\n    created: async function () {\r\n        await this.verificarLog();\r\n        await this.loadUserSelect();\r\n        await this.$root.validarLoginFooter.call();\r\n    }\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n</style>\r\n\r\n<style scoped>\r\n\r\n#header {\r\n    margin: auto;\r\n    width: 500px;\r\n    font-family: Arial, Helvetica, sans-serif;\r\n}\r\n\r\nul, ol {\r\n    list-style: none;\r\n}\r\n\r\n.nav > li {\r\n    float: left;\r\n}\r\n\r\n.nav li a {\r\n    background-color: #262b35;\r\n    color: #fff;\r\n    text-decoration: none;\r\n    padding: 10px 12px;\r\n    display: block;\r\n}\r\n\r\n    .nav li a:hover {\r\n        color: #808080;\r\n        background-color: #262b35;\r\n    }\r\n\r\n.nav li ul {\r\n    display: none;\r\n    position: absolute;\r\n    min-width: 140px;\r\n}\r\n\r\n.nav li:hover > ul {\r\n    display: block;\r\n}\r\n\r\n.nav li ul li {\r\n    position: relative;\r\n}\r\n\r\n.nav li ul li ul {\r\n    right: -140px;\r\n    top: